FROM     SanPham AS SP INNER JOIN
                  ChiTietHoaDon AS CT ON SP.maSP = CT.maSP INNER JOIN
                  HoaDon AS HD ON CT.maHD = HD.maHD INNER JOIN
                  KhachHang AS KH ON HD.maKH = KH.maKH INNER JOIN
